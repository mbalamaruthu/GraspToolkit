            name="Syntax Coloring">
            <keywordReference id="uk.ac.standrews.graspide.grasp.ui.keyword_GraspDSL"/>
        </page>
        <page
            category="uk.ac.standrews.graspide.grasp.GraspDSL"
            class="uk.ac.standrews.graspide.grasp.ui.GraspDSLExecutableExtensionFactory:uk.ac.standrews.graspide.grasp.ui.ValidationPreferencesPage"
